{
  "name": "Delete Block Storage",
  "priority": 10,
  "request": {
    "method": "DELETE",
    "urlPathPattern": "/v1/tenants/[^/]+/workspaces/[^/]+/block-storages/[^/]+",
    "headers": {
      "Authorization": {
        "matches": "Bearer .+"
      }
    }
  },
  "response": {
    "status": 202,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "message": "Block-Storage deletion accepted."
    }
  }
}