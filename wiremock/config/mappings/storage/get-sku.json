{
  "name": "Get Storage SKU",
  "priority": 11,
  "request": {
    "method": "GET",
    "urlPathPattern": "/providers/seca.storage/v1/tenants/[^/]+/skus/[^/]+",
    "headers": {
      "Accept": {
        "equalTo": "application/json"
      },
      "Authorization": {
        "matches": "Bearer test-token"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "spec": {
        "provider": "SECA",
        "type": "remote-durable",
        "iops": 100,
        "minVolumeSize": 1
      },
      "metadata": {
        "name": "{{request.pathSegments.[6]}}",
        "createdAt": "2025-07-22T10:10:00Z",
        "lastModifiedAt": "2025-07-22T10:10:00Z",
        "resourceVersion": 1,
        "tenant": "{{request.pathSegments.[5]}}",
        "region": "eu-central-1",
        "apiVersion": "v1",
        "kind": "storage-sku",
        "resource": "seca.storage/skus/{{request.pathSegments.[6]}}",
        "verb": "get"
      }
    }
  }
}
