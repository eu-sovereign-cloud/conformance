{
  "name": "List Storage SKU",
  "priority": 10,
  "request": {
    "method": "GET",
    "urlPathPattern": "/providers/seca.storage/v1/tenants/([^/]+)/skus",
    "headers": {
      "Authorization": {
        "matches": "Bearer test-token"
      }
    }
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "jsonBody": {
      "items": [
        {
          "labels": {
            "provider": "seca",
            "tier": "RD100"
          },
          "metadata": {
            "name": "seca.rd100",
            "provider": "seca.storage/v1",
            "resource": "seca.storage/v1/skus/seca.rd100",
            "verb": "get",
            "createdAt": "2025-01-01T09:00:00Z",
            "lastModifiedAt": "2025-01-02T10:00:00Z",
            "resourceVersion": 1,
            "apiVersion": "v1",
            "kind": "storage-sku",
            "tenant": "{{request.pathSegments.[4]}}",
