          },
          "spec": {
            "iops": 100,
            "minVolumeSize": 50,
            "type": "remote-durable"
          }
        }
      ],
      "metadata": {
        "provider": "seca.storage/v1",
        "resource": "skus",
        "verb": "list"
      }
    }
  }
}
