{
    "name": "restart-instance",
    "priority": 10,
    "request": {
        "method": "POST",
        "urlPathPattern": "/providers/seca.compute/v1/tenants/([^/]+)/workspaces/([^/]+)/instances/([^/]+)/power-cycle",
        "headers": {
        "Authorization": {
            "matches": "Bearer .+"
        }
        }
    },
    "response": {
        "status": 202,
        "headers": {
        "Content-Type": "application/json"
        },
        "jsonBody": {
        "message": "Instance power cycle accepted."
        }
    }
}